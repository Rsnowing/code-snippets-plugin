{
	"手机号": {
		"prefix": "phone-手机号",
		"body": ["/^[1][3,4,5,7,8][0-9]{9}$/"],
		"description": "手机号正则"
  },
  "身份证": {
    "prefix": "idcard",
    "body": ["/^[1-9]\\d{5}(18|19|([23]\\d))\\d{2}((0[1-9])|(10|11|12))(([0-2][1-9])|10|20|30|31)\\d{3}[0-9Xx]$/"],
    "description": "身份证"
  },
  "email邮箱": {
    "prefix": "email",
    "body": ["/^[A-Za-z\\d]+([-_.][A-Za-z\\d]+)*@([A-Za-z\\d]+[-.])+[A-Za-z\\d]{2,4}$/"],
    "description": "email"
  },
  "微信号": {
    "prefix": "wx",
    "body": ["/^[a-zA-Z]([-_a-zA-Z0-9]{5,19})+$/"]
  }
}
