{
	"name": "reg-collect",
	"displayName": "ax-reg-collect",
	"description": "正则收集",
	"version": "0.0.2",
	"publisher": "llhe",
	"icon": "./image/logo.jpg",
	"engines": {
		"vscode": "^1.51.0"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"categories": [ "Snippets" ],
	"activationEvents": [ "onCommand:reg-collect.ax-reg" ],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "reg-collect.ax-reg",
				"title": "ax-reg"
			}
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets.json"
			},
			{
				"language": "html",
				"path": "./snippets.json"
			},
			{
				"language": "vue",
				"path": "./snippets.json"
			},
			{
				"language": "ts",
				"path": "./snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
